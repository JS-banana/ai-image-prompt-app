{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "build:turbo": "next build --turbo",
    "start": "next start",
    "lint": "pnpm exec eslint .",
    "lint:fix": "pnpm exec eslint . --fix",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "typecheck": "pnpm exec tsc --noEmit -p tsconfig.json",
    "typecheck:tests": "pnpm exec tsc --noEmit -p tsconfig.test.json",
    "db:seed": "tsx prisma/seed.ts",
    "db:import:banana": "tsx scripts/import-banana-prompts.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ark": "tsx scripts/ark-smoketest.ts",
    "e2e": "playwright test"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.19.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "next": "16.0.7",
    "prisma": "5.19.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "msw": "^2.12.4",
    "prettier": "^3.4.2",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^2.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "pnpm exec eslint --fix"
    ],
    "*.{md,mdx,json,css,scss}": [
      "pnpm exec prettier --cache --write"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.23.0"
}
